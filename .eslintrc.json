{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "max-len": ["warn", {
            "code": 120
        }],
        "newline-per-chained-call": "off",
        "no-console": "off",
        "object-curly-newline": "off"
    }
}
